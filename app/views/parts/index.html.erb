<h1>Listing parts</h1>

<table>
  <tr>
    <th>Part number</th>
    <th>Name</th>
    <th>Prodict</th>
    <th>Weight</th>
    <th>Dimensions</th>
    <th>Supplier</th>
    <th>Delivery date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @parts.each do |part| %>
  <tr>
    <td><%= part.part_number %></td>
    <td><%= part.name %></td>
    <td><%= part.prodict_id %></td>
    <td><%= part.weight %></td>
    <td><%= part.dimensions %></td>
    <td><%= part.supplier_id %></td>
    <td><%= part.delivery_date %></td>
    <td><%= link_to 'Show', part %></td>
    <td><%= link_to 'Edit', edit_part_path(part) %></td>
    <td><%= link_to 'Destroy', part, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Part', new_part_path %>
